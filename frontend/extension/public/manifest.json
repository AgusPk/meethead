{
  "name": "webpack test",
  "description": "The power of React and TypeScript for building interactive Chrome extensions",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "128": "favicon.ico"
  },
  "permissions": ["activeTab", "webNavigation", "webRequest", "storage", "tabs"],
  "host_permissions": ["http://www.linkedin.com/"],
  "background": {
    "service_worker": "worker-wrapper.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["./assets/js/content.bundle.js"],
      "css": ["./assets/css/content.9b484566557db4e8d8d7.css"]
    }
  ]
}
